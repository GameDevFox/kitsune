// -*- mode: js2 -*-
export default function bind({ func, params }) {
    let f = function(partParams) {
        let fullParams = {};
        for(let key in params)
            fullParams[key] = params[key];

        for(let key in partParams)
            fullParams[key] = partParams[key];

        return func(fullParams);
    };
    return f;
}
module.exports = bind;
