// -*- mode: js2 -*-
export default function removeName({ node, name, stringGetId, graphFactor, graphRemove }) {
    let strId = stringGetId({ string: name });

    if(!strId)
        return;

    let fac = graphFactor({ head: strId, tail: node });
    if(fac.length === 0)
        return;

    fac = fac[0];

    graphRemove(fac.id);
    graphRemove(fac.typeEdge);
}
module.exports = removeName;
